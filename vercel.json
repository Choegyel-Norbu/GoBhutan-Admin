{
  "redirects": [
    {
      "source": "/",
      "destination": "/go-bhutan-admin/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/go-bhutan-admin/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/go-bhutan-admin/icon.svg",
      "destination": "/icon.svg"
    },
    {
      "source": "/go-bhutan-admin/manifest.json",
      "destination": "/manifest.json"
    },
    {
      "source": "/go-bhutan-admin/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/go-bhutan-admin",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/go-bhutan-admin/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
